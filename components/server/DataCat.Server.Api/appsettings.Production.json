{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DataSourceType": "postgres",
  "ApplyMigrations": true,
  "DatabaseOptions": {
    "ConnectionString": "Server=localhost;Port=5433;Database=db;User Id=postgres;Password=postgres;",
    "Scheme": "public",
    "TimeoutInSeconds": 60
  },
  "PluginStoreOptions": {
    "PluginPath": "uploads"
  }
}
