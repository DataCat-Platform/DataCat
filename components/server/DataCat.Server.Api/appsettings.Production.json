{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "DataSourceType": "postgres",
  "ApplyMigrations": true,
  "DatabaseOptions": {
    "ConnectionString": "Server=localhost;Port=5433;Database=db;User Id=postgres;Password=postgres;",
    "Scheme": "public",
    "TimeoutInSeconds": 60
  },
  "SecretsStorageType": "in-memory",
  "AuthType": "default",
  "AuthMappingOptions": {
    "admin": "Admin",
    "viewer": "Viewer",
    "editor": "Editor"
  },
  "PluginStoreOptions": {
    "PluginPath": "uploads"
  }
}
